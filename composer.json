{
    "name": "protonemedia/laravel-mixins",
    "description": "Laravel Mixins",
    "keywords": [
        "protonemedia",
        "laravel-mixins"
    ],
    "homepage": "https://github.com/protonemedia/laravel-mixins",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "html2text/html2text": "^4.3",
        "laravel/ui": "^1.0 || ^2.0",
        "moneyphp/money": "^3.3",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.5",
        "spatie/laravel-sitemap": "^5.7",
        "symfony/process": "^5.1"
    },
    "suggest": {
        "html2text/html2text": "To convert HTML to formatted plain text",
        "moneyphp/money": "Blade directives to represent money",
        "spatie/laravel-sitemap": "To generate sitemaps",
        "symfony/process": "To run Ghostscript"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\LaravelMixins\\MixinsServiceProvider"
            ]
        }
    }
}