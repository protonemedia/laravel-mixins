{
    "name": "protonemedia/laravel-mixins",
    "description": "Laravel Mixins",
    "keywords": [
        "protonemedia",
        "laravel-mixins"
    ],
    "homepage": "https://github.com/protonemedia/laravel-mixins",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "html2text/html2text": "^4.3",
        "jeremykendall/php-domain-parser": "^6.0",
        "laravel/ui": "^3.0",
        "mockery/mockery": "^1.3.3",
        "moneyphp/money": "^3.3",
        "nesbot/carbon": "^2.63",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-sitemap": "^6.0",
        "symfony/process": "^6.0"
    },
    "suggest": {
        "html2text/html2text": "To convert HTML to formatted plain text",
        "moneyphp/money": "Blade directives to represent money",
        "spatie/laravel-sitemap": "To generate sitemaps",
        "symfony/process": "To run Ghostscript"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
