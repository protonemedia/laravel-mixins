{
    "name": "protonemedia/laravel-mixins",
    "description": "Laravel Mixins",
    "keywords": [
        "protonemedia",
        "laravel-mixins"
    ],
    "homepage": "https://github.com/protonemedia/laravel-mixins",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4 || ^8.5",
        "illuminate/support": "^11.0 || ^12.0 || ^13.0"
    },
    "require-dev": {
        "html2text/html2text": "^4.3",
        "jeremykendall/php-domain-parser": "^6.0",
        "laravel/ui": "^4.0",
        "mockery/mockery": "^1.4.4",
        "moneyphp/money": "^3.3 || ^4.5",
        "nesbot/carbon": "^2.71 || ^3.0",
        "orchestra/testbench": "^9.10 || ^10.0 || ^11.0",
        "phpunit/phpunit": "^10.4 || ^11.5.3 || ^12.5.12",
        "spatie/laravel-sitemap": "^6.0 || ^7.2",
        "symfony/process": "^6.0 || ^7.0"
    },
    "suggest": {
        "html2text/html2text": "To convert HTML to formatted plain text",
        "moneyphp/money": "Blade directives to represent money",
        "spatie/laravel-sitemap": "To generate sitemaps",
        "symfony/process": "To run Ghostscript"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}